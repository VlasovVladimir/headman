        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:headman.Forms.MapChoise"
        mc:Ignorable="d"
        Title="Я хочу сыграть с тобой в игру. Выбирай." Height="400" Width="600">
    <Grid>
        <ListBox x:Name="MapsList" HorizontalAlignment="Left" Height="350" Margin="25,10,0,0" VerticalAlignment="Top" Width="250" IsEnabled="False"/>
        <Image x:Name="MapPicture" HorizontalAlignment="Left" Height="225" Margin="310,75,0,0" VerticalAlignment="Top" Width="255"/>

    </Grid>
</Window>
