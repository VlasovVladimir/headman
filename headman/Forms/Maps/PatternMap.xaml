        <TextBox x:Name="WaterInfo" HorizontalAlignment="Left" Height="30" Margin="1170,649,0,0" TextWrapping="Wrap" Text="Сколько-та там" VerticalAlignment="Top" Width="50" FontSize="18.667" IsEnabled="False"/>
        <Button x:Name="Get0" Content="✔" HorizontalAlignment="Left" Margin="1190,765,0,0" VerticalAlignment="Top" Width="30"/>
        <Button x:Name="Get1" Content="✔" HorizontalAlignment="Left" Margin="1190,792,0,0" VerticalAlignment="Top" Width="30"/>
        <Button x:Name="Get2" Content="✔" HorizontalAlignment="Left" Margin="1190,819,0,0" VerticalAlignment="Top" Width="30"/>
        <Button x:Name="Get3" Content="✔" HorizontalAlignment="Left" Margin="1190,847,0,0" VerticalAlignment="Top" Width="30"/>
