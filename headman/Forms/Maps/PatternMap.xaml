        <TextBox x:Name="Thing3" HorizontalAlignment="Left" Height="25" Margin="950,847,0,0" TextWrapping="Wrap" Text="Комплект кирок" VerticalAlignment="Top" Width="185" FontSize="16" IsEnabled="False"/>
        <TextBox x:Name="Thing4" HorizontalAlignment="Left" Height="25" Margin="950,875,0,0" TextWrapping="Wrap" Text="Крыша над головой" VerticalAlignment="Top" Width="185" FontSize="16" IsEnabled="False"/>
        <TextBox x:Name="Thing5" HorizontalAlignment="Left" Height="25" Margin="950,903,0,0" TextWrapping="Wrap" Text="Комплект копий" VerticalAlignment="Top" Width="185" FontSize="16" IsEnabled="False"/>
