<Window x:Class="headman.Forms.Maps.PatternMap"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PatternMap" Height="1024" Width="1280" ResizeMode="NoResize">
    <Window.Background>
        <ImageBrush ImageSource="/headman;component/Images/Image1.jpg" Stretch="UniformToFill" TileMode="None"  />
    </Window.Background>
    <Grid>
        <Frame x:Name="Base" Content="" HorizontalAlignment="Left" Height="900" VerticalAlignment="Top" Width="855" Foreground="#FFEABBBB" Background="#FFFBF6F6" Margin="40,45,0,0"/>
        <TextBlock x:Name="Log" HorizontalAlignment="Left" Height="510" Margin="940,45,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="295" Background="#FFE2E0E0"/>
        <Button x:Name="Start" Content="►" HorizontalAlignment="Left" Height="70" Margin="120,875,0,0" VerticalAlignment="Top" Width="75" FontSize="48"/>
        <Button x:Name="Pause" Content="▌▐" HorizontalAlignment="Left" Height="70" Margin="40,875,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="1.933,0.5" FontSize="32"/>
        <Button x:Name="SpeedUp" Content="►►" HorizontalAlignment="Left" Height="70" Margin="200,875,0,0" VerticalAlignment="Top" Width="75" FontSize="29.333"/>
        <TextBox x:Name="Timing" HorizontalAlignment="Left" Height="45" Margin="305,900,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="150" IsEnabled="False"/>
        <Button x:Name="GoTo" Content="✈" HorizontalAlignment="Left" Height="70" Margin="820,875,0,0" VerticalAlignment="Top" Width="75" FontSize="48"/>
        <Frame x:Name="RowResources" Content="" HorizontalAlignment="Left" Height="120" Margin="940,575,0,0" VerticalAlignment="Top" Width="295" Background="#FFACA6A6"/>
        <Frame x:Name="Things" Content="" HorizontalAlignment="Left" Height="230" Margin="940,715,0,0" VerticalAlignment="Top" Width="295" Background="Gray"/>
        <TextBox x:Name="HeadResources" HorizontalAlignment="Left" Height="30" Margin="950,590,0,0" TextWrapping="Wrap" Text="Ресурсы" VerticalAlignment="Top" Width="90" FontSize="18.667" IsEnabled="False"/>
        <TextBox x:Name="HeadResources_Copy" HorizontalAlignment="Left" Height="30" Margin="950,730,0,0" TextWrapping="Wrap" Text="Предметы" VerticalAlignment="Top" Width="130" FontSize="18.667" IsEnabled="False"/>
        <TextBlock x:Name="Wooden" HorizontalAlignment="Left" Height="40" Margin="1045,642,0,0" TextWrapping="Wrap" Text="✿" VerticalAlignment="Top" Width="40" FontSize="32" IsEnabled="False"/>
        <TextBlock x:Name="Water" HorizontalAlignment="Left" Height="40" Margin="1145,639,0,0" TextWrapping="Wrap" Text="☂" VerticalAlignment="Top" Width="40" RenderTransformOrigin="0.5,1.833" FontSize="32" IsEnabled="False"/>
