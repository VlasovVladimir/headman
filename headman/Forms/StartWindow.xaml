<Window x:Class="headman.Forms.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:headman.Forms"
        mc:Ignorable="d"
        Title="Здравствуйте!" Height="460" Width="535" WindowStartupLocation="CenterScreen" WindowStyle="None">
    <Window.Background>
        <ImageBrush ImageSource="/headman;component/Images/start_boat.jpg" Stretch="UniformToFill" TileMode="None"  />
    </Window.Background>
    <Grid>
        <TextBox x:Name="Info" HorizontalAlignment="Left" Height="215" Margin="25,25,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="470" IsEnabled="False" FontSize="17"/>
        <Button x:Name="Go" Content="Вперед!" HorizontalAlignment="Left" Height="35" Margin="190,365,0,0" VerticalAlignment="Top" Width="140" Click="button_Click"/>

    </Grid>
</Window>
